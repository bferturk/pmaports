pkgname=osso-icons
pkgver=2.5.0
pkgrel=0
pkgdesc="OSSO UI icons"
url="https://github.com/maemo-leste/osso-icons"
arch="noarch"
license="proprietary"
source="$pkgname-$pkgver.tar.gz::https://github.com/maemo-leste/$pkgname/archive/$pkgver.tar.gz"
options="!check"

package() {
	cd "$builddir"/files
	find . -type f -exec install -D -m 644 {} "$pkgdir"/{} \;
	rm -rf "$pkgdir"/usr/share/doc
}

